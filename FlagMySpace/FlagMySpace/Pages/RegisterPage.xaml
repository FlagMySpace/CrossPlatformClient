<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FlagMySpace.Pages.RegisterPage">
  <StackLayout>
    <Entry x:Name="EntryUsername" Placeholder="username"></Entry>
    <Entry x:Name="EntryPassword" Placeholder="password" IsPassword="True"></Entry>
    <Entry x:Name="EntryConfirmPassword" Placeholder="confirm password" IsPassword="True"></Entry>
    <Entry x:Name="EntryEmail" Placeholder="email"></Entry>
    <Button Text="login"></Button>
    <Button x:Name="ButtonRegister"></Button>
  </StackLayout>
</ContentPage>